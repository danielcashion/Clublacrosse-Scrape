{
	"name": "ImportLUStatisticTypes",
	"properties": {
		"activities": [
			{
				"name": "StageStatisticNames",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 3,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "FileSystemSource",
						"recursive": true
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000,
						"preCopyScript": "TRUNCATE TABLE [casp].[lax_lu_statistic_names]"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"statistic_id": "statistic_id",
							"sport_id": "sport_id",
							"statistic_name": "statistic_name",
							"is_active": "is_active",
							"created_by": "created_by",
							"created_datetime": "created_datetime"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "AWSBucketLUStatisticTypes",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureTableLUStatisticTypes",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Import Caspio Data"
		}
	}
}